#include <iostream>
using namespace std;

int main() {
    
    int A;
    
    cout << "Program Kedewasaan \n\n";
    cout << "Masukkan umur anda = \n";
    
    cin >> A;
    
    if(A <= 5) {
        cout << "maka anda adalah Anak-anak \n";
    } else if(A <= 17) {
        cout << "maka anda adalah Remaja \n";
    } else {
        cout << "maka anda adalah Dewasa \n";
    }
    
    return 0;
}
